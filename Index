            <span>–ì–ª–∞–≤–Ω–∞—è</span>
        </button>
        <button class="nav-btn" onclick="showTab('cheats')">
            <i class="fas fa-gamepad"></i>
            <span>–ß–∏—Ç—ã</span>
        </button>
        <button class="nav-btn" onclick="showTab('admin')">
            <i class="fas fa-cogs"></i>
            <span>–ê–¥–º–∏–Ω</span>
        </button>
        <button class="nav-btn" onclick="showTab('stats')">
            <i class="fas fa-chart-bar"></i>
            <span>–°—Ç–∞—Ç—ã</span>
        </button>
    </div>

    <!-- –°—Ç–∞—Ç—É—Å –±–∞—Ä -->
    <div class="status-bar" id="statusBar">
        <div class="status-dot"></div>
        <span id="statusMessage">–ì–æ—Ç–æ–≤–æ</span>
    </div>

    <script>
        // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏
        function updateSpeed(value) {
            document.getElementById('speedValue').textContent = value;
        }

        // –ü–æ–∫–∞–∑–∞—Ç—å —Å—Ç–∞—Ç—É—Å
        function showStatus(message, type = 'success') {
            const statusBar = document.getElementById('statusBar');
            const statusMessage = document.getElementById('statusMessage');
            
            statusMessage.textContent = message;
            statusBar.style.borderColor = type === 'error' ? 'var(--danger)' : 
                                        type === 'warning' ? '#fdcb6e' : 'var(--success)';
            statusBar.style.display = 'flex';
            
            setTimeout(() => {
                statusBar.style.display = 'none';
            }, 3000);
        }

        // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–∫—Ä–∏–ø—Ç–∞
        async function updateScript() {
            const code = document.getElementById('scriptCode').value;
            const version = document.getElementById('scriptVersion').value;
            
            if (!code.trim()) {
                showStatus('–í–≤–µ–¥–∏—Ç–µ Lua –∫–æ–¥!', 'error');
                return;
            }
            
            showStatus(`–û–±–Ω–æ–≤–ª—è—é —Å–∫—Ä–∏–ø—Ç –¥–æ –≤–µ—Ä—Å–∏–∏ ${version}...`, 'warning');
            
            // –ó–¥–µ—Å—å –±—É–¥–µ—Ç –∑–∞–≥—Ä—É–∑–∫–∞ –Ω–∞ GitHub
            try {
                // –ó–∞–≥–ª—É—à–∫–∞ –¥–ª—è GitHub API
                await new Promise(resolve => setTimeout(resolve, 1500));
                showStatus(`–°–∫—Ä–∏–ø—Ç v${version} —É—Å–ø–µ—à–Ω–æ –æ–±–Ω–æ–≤–ª–µ–Ω!`);
                document.getElementById('currentVersion').textContent = version;
                document.getElementById('lastUpdate').textContent = new Date().toLocaleTimeString();
            } catch (error) {
                showStatus('–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–∏!', 'error');
            }
        }

        // –û—Ç–ø—Ä–∞–≤–∫–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è
        function sendNotification() {
            const title = document.getElementById('notifTitle').value;
            const text = document.getElementById('notifText').value;
            
            if (!title.trim() || !text.trim()) {
                showStatus('–ó–∞–ø–æ–ª–Ω–∏—Ç–µ –≤—Å–µ –ø–æ–ª—è!', 'error');
                return;
            }
            
            showStatus(`–£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ "${title}" –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ!`);
            
            // –û—á–∏—Å—Ç–∫–∞ –ø–æ–ª–µ–π
            document.getElementById('notifTitle').value = '';
            document.getElementById('notifText').value = '';
        }

        // –ë–∞–Ω –∏–≥—Ä–æ–∫–∞
        function banPlayer() {
            const player = document.getElementById('playerName').value;
            const reason = document.getElementById('banReason').value;
            const details = document.getElementById('banDetails').value;
            
            if (!player.trim()) {
                showStatus('–í–≤–µ–¥–∏—Ç–µ –∏–º—è –∏–≥—Ä–æ–∫–∞!', 'error');
                return;
            }
            
            showStatus(`–ò–≥—Ä–æ–∫ ${player} –∑–∞–±–∞–Ω–µ–Ω –ø–æ –ø—Ä–∏—á–∏–Ω–µ: ${reason}`, 'warning');
            
            // –û—á–∏—Å—Ç–∫–∞ –ø–æ–ª–µ–π
            document.getElementById('playerName').value = '';
            document.getElementById('banDetails').value = '';
        }

        // –ü–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–µ –≤—Å–µ—Ö —á–∏—Ç–æ–≤
        function toggleAllCheats() {
            const toggle = document.getElementById('toggleAllCheats');
            const status = toggle.checked ? '–≤–∫–ª—é—á–µ–Ω—ã' : '–≤—ã–∫–ª—é—á–µ–Ω—ã';
            showStatus(`–í—Å–µ —á–∏—Ç—ã ${status}`);
        }

        // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        function refreshStats() {
            // –ò–º–∏—Ç–∞—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
            const online = Math.floor(Math.random() * 100) + 50;
            document.getElementById('onlineCount').textContent = online;
            showStatus('–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∞!');
        }

        // –ú–æ–±–∏–ª—å–Ω—ã–µ —Ç–∞–±—ã
        function showTab(tabName) {
            document.querySelectorAll('.nav-btn').forEach(btn => btn.classList.remove('active'));
            event.currentTarget.classList.add('active');
            showStatus(`–ü–µ—Ä–µ—Ö–æ–¥ –≤ —Ä–∞–∑–¥–µ–ª: ${tabName}`);
        }

        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
        document.addEventListener('DOMContentLoaded', function() {
            // –ó–∞–≥—Ä—É–∑–∫–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–Ω–æ–≥–æ –∫–æ–¥–∞ (–∑–∞–≥–ª—É—à–∫–∞)
            const savedCode = localStorage.getItem('ultimate_script');
            if (savedCode) {
                document.getElementById('scriptCode').value = savedCode;
            }
            
            // –ê–≤—Ç–æ—Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ
            document.getElementById('scriptCode').addEventListener('input', function() {
                localStorage.setItem('ultimate_script', this.value);
            });
            
            // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –≤—Ä–µ–º–µ–Ω–∏
            setInterval(() => {
                const time = new Date().toLocaleTimeString();
                document.getElementById('lastUpdate').textContent = `–°–µ–≥–æ–¥–Ω—è, ${time}`;
            }, 60000);
            
            // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –ø—Ä–∏–≤–µ—Ç—Å—Ç–≤–∏–µ
            setTimeout(() => {
                showStatus('–ê–¥–º–∏–Ω-–ø–∞–Ω–µ–ª—å –≥–æ—Ç–æ–≤–∞ –∫ —Ä–∞–±–æ—Ç–µ!');
            }, 1000);
        });
    </script>
</body>
</html>
